{
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],

    "require": {
        "johnpbloch/wordpress": "^5.8.2",
        "wpackagist-theme/twentynineteen": "*",
        "wpackagist-plugin/acf-contact-form-7":"1.1.6",
        "wpackagist-plugin/contact-form-7":"5.4.1",
        "wpackagist-plugin/advanced-custom-fields": "5.11.1",
        "wpackagist-plugin/wp-mail-bank":"4.0.14",
        "wpackagist-plugin/wp-stats-manager":"4.5",
        "wpackagist-plugin/wp-maintenance-mode":"2.4.1"
    },
    
    "require-dev": {
        "wpackagist-plugin/query-monitor":"3.5.2",
        "wpackagist-plugin/rewrite-rules-inspector": "1.3.1",
        "wpackagist-plugin/what-the-file":"1.5.4"

    },

    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "content/themes/{$name}": ["type:wordpress-theme"],
            "content/plugins/{$name}": ["type:wordpress-plugin"]
        }
    }
}