{
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],

    "require": {
        "johnpbloch/wordpress": "^5.3.2",
        "wpackagist-theme/twentynineteen": "*",
        "wpackagist-plugin/acf-contact-form-7":"1.1.6",
        "wpackagist-plugin/contact-form-7":"5.1.6",
        "wpackagist-plugin/advanced-custom-fields": "5.8.7"
    },
    
    "require-dev": {
        "wpackagist-plugin/query-monitor":"3.5.2",
        "wpackagist-plugin/simply-show-hooks":"1.2.1",
        "wpackagist-plugin/rewrite-rules-inspector": "1.2.1"

    },

    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "content/themes/{$name}": ["type:wordpress-theme"],
            "content/plugins/{$name}": ["type:wordpress-plugin"]
        }
    }
}